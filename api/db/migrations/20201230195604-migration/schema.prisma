datasource DS {
  // optionally set multiple providers
  // example: provider = ["sqlite", "postgresql"]
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  Task  Task[]
}

model Task {
  id      Int    @id @default(autoincrement())
  title   String
  owner   User   @relation(fields: [ownerId], references: [id])
  ownerId Int
}
